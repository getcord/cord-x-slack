    "@slack/web-api": "^6.8.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "envfile": "^6.18.0",
    "express": "^4.18.2",
    "fast-json-stable-stringify": "^2.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.9",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
